/reports\";\r\n        }catch (err){\r\n            console.error(err.message);\r\n        }\r\n    };\r\n\r\n    const getReports = async () => {\r\n        try{\r\n            const response = await fetch(\"http://localhost:5000/flags\");\r\n            const jsonData = await response.json();\r\n\r\n            setReports(jsonData);\r\n        }catch (err){\r\n            console.error(err.message);\r\n        }\r\n    };\r\n\r\n\r\n    useEffect(() => {\r\n        getReports();\r\n    }, []);\r\n\r\n    return <Fragment>\r\n        {\" \"}\r\n        <table className=\"table mt-5 text-center\">\r\n          <thead>\r\n            <tr>\r\n              <th>Flag ID</th>\r\n              <th>User ID</th>\r\n              <th>Post ID</th>\r\n              <th>Post Type</th>\r\n              <th>Link</th>\r\n              <th>Delete Flag</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            {reports.map(report => (\r\n                    <tr key={report.flag_id}>\r\n                        <td>{report.flag_id}</td>\r\n                        <td>{report.user_id}</td>\r\n                        <td>{report.post_id}</td>\r\n                        <td>{(report.post_type) ? \"Review\":\"Description\" }</td>\r\n                        <td>\r\n                          <PostModal r = {report}/>\r\n                        </td>\r\n                        <td>\r\n                        <button \r\n                            className=\"btn btn-danger\" \r\n                            onClick = {() => deleteReport(report.flag_id)}\r\n                        >\r\n                            Delete Flag\r\n                        </button>\r\n                        </td>\r\n                    </tr>\r\n                ))}\r\n          </tbody>\r\n        </table>\r\n    </Fragment>\r\n};\r\n\r\nexport default ListReports;","C:\\Users\\glgre\\Documents\\School\\CS\\CSCI3341\\webApp\\ClassRater\\client\\src\\components\\BanUser.js",["91","92","93"